# Uncomment the next line to define a global platform for your project
# platform :ios, '9.0'

target 'BambooTreeForest' do
  # Comment the next line if you don't want to use dynamic frameworks
  use_frameworks!

  # Firebase Core 라이브러리 추가 (필수)
  pod 'FirebaseCore'

  # 필요한 Firebase 모듈 추가
  pod 'FirebaseFirestore'  # Firestore 사용 시
  pod 'FirebaseAuth'       # Authentication 사용 시
  pod 'FirebaseAnalytics'  # Analytics 사용 시

  # Pods for BambooTreeForest

end
